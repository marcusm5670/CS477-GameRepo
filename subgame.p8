--player


// creates player object with
// its own properties
function make_player()
 p={
  --physics
  x=40, // starting x coord
  y=40, // starting y coord
  dx=0, // horizontal move speed
  dy=0, // vertical move speed
  max_dx=1,
  max_dy=1,
  max_⬆️dy=4,
  acc=0.8,
  --stuff & gadgets
  money=10000,
  fuel=80,
  max_fuel=100,
  health=100,
  ore=100,
  torpedos=300,
  charges=100,
  eq=0,
  --dimensions
  w=8,
  h=5,
  sp=0,
  flp=false
 }
 
end

// draw player sprite at start
// position, (p.x, p.y)
function draw_player()
 if p.flp==false then 
  spr(p.sp,p.x,p.y)
 else
  spr(1,p.x,p.y)
 end
end

// player can move left, right,
// up, and down
function update_player()
 
 p.dx*=friction
 p.dy*=friction
 
 if btn(⬅️) and
 collide_map(p,"left",0)==false
 then
  p.dx-=p.acc
  p.fuel-=0.04
  p.flp=true
 end
 
 if btn(➡️) and
 collide_map(p,"right",0)==false
 then 
  p.dx+=p.acc
  p.fuel-=0.04 
  p.flp=false
 end
 
 if btn(⬆️) and
 collide_map(p,"up",0)==false
 then 
  p.dy-=4*p.acc
  p.fuel-=0.04
 end
 
 if btn(⬇️) and
 collide_map(p,"down",0)==false
 then 
  p.dy+=p.acc
  p.fuel-=0.04
 end
 
 if btnp(4)then
  if p.eq==1 and
  p.torpedos!=0 then
   make_torp(p.x,p.y)
   p.torpedos-=1
  
  elseif p.eq==2 and
  p.charges!=0 then
   make_charge(p.x,p.y)
   p.charges-=1
  end
  
 end
 
 if btnp(❎) then
  if p.eq!=2 then
   p.eq+=1
  else 
   p.eq=0
  end
 end
 // moves the player
 
 p.x+=p.dx
 p.y+=p.dy
 
 if rnd(1)<0.5 then
  make_bubble(p.x,p.y)
 end
 
 // check horizontal collision
 if p.dx<0 then
  if collide_map(p,"left",0)then
   p.dx=0 
   
  else 
   p.dx=lim_speed(p.dx,p.max_dx)
  end
  
 elseif p.dx>0 then
  if collide_map(p,"right",0)then
   p.dx=0 
   
  else
   p.dx=lim_speed(p.dx,p.max_dx)
  end
  
 end
 
 // check vertical collision
 if p.dy<0 then
  p.dy=lim_speed(p.dy,p.max_dy)
  
  if collide_map(p,"up",0)then
   p.dy=0 
  end
 
 elseif p.dy>0 then
  p.dy=lim_speed(p.dy,p.max_dy)
  
  if collide_map(p,"down",0)then
   p.dy=0
  end
 end
 --shop
 if collide_map(p,"up",3) then
  shoptouch=true
  --scene=2
 
 end
end // end move_player

function lim_speed(num,maximum)
 return mid(-maximum,num,maximum)
end

