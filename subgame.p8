--enemies
function setup_enemies()
 enemies={}
end

function make_enemy(x,y)
 local enemy={}
 
 enemy.x=x
 enemy.y=y
 
 enemy.w=16
 enemy.h=16
 
 enemy.dx=1
 enemy.dy=1
 
 enemy.spr=34
 
 add(enemies,enemy)
end

function update_enemy(e)
 if p.x>e.x then
  e.x+=e.dx
 else
  e.x-=e.dx
 end
 
 if p.y>e.x then
  e.y+=e.dy
 else
  e.y-=e.dy
 end
end

function draw_enemy(e)
 spr(enemy.spr,enemy.x,enemy.y,2,2)
end
