version 38
__lua__
--game loop

// draw player on top of map
function _init()
 scene=0
 friction=0.75
 
 torps_setup()
 charges_setup()
 bubbles_setup()
 map_setup()
 make_player()
 make_enemy(90,90)
 
end

// move camera with player
function _update()
 --main menu
 if scene==0 then
  update_menu()
 end
 
 --game
 if scene==1 then
  foreach(torps,update_torp)
  foreach(charges,update_charge)
  foreach(enemies,update_enemy)
  foreach(bubbles,update_bubble)
  update_player()
  camera(p.x-53, p.y-53)
 end
 
 --shop
 if scene==2 then
  update_menu()
 end

end

// draw player and map sprites
function _draw()
 if scene==0 then
  draw_menu() 
 end
 
 if scene==1 then
  cls()
  draw_map()
  foreach(torps,draw_torp)
  foreach(charges,draw_charge)
  foreach(bubbles,draw_bubble)
  foreach(enemies,draw_enemy)
  draw_player()
  draw_fuel_hud()
  draw_gadget_hud()
 end
 
 if shoptouch==true then
   for i=0,90 do
    print('cant use shop yet',
    p.x,p.y-20,7)
   end
   shoptouch=false
 end
end
