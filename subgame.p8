--map

// map sprites
function map_setup()
 wall = 0
 mineral = 1
 treasure = 2
end

// draw entire map
function draw_map()
 map(0,0,0,0,128,64)
end

// some code taken from 
// nerdyteachers.com
//
// allows an object to move if
// it's not trying to move onto
// a wall sprite
function collide_map(obj,aim,flag)
 
 // object's dimensions
 local x=obj.x
 local y=obj.y
 local w=obj.w
 local h=obj.h
 
 // coordinates of the corners
 // of obj's hitbox
 local x1=0	 // x1,y1---x2,y1 
 local x2=0	 //   |							|
 local y1=0	 //   |							|
 local y2=0	 // x1,y2---x2,y2
 
 // coords left of hitbox
 if aim=="left" then
  x1=x-1			
  y1=y
  x2=x				
  y2=y+h-1
 
 // coords right of hitbox
 elseif aim =="right" then
  x1=x+w
  y1=y
  x2=x+w+1
  y2=y+h-1
 
 // coords above hitbox
 elseif aim =="up" then
  x1=x+1
  y1=y-1
  x2=x+w-1
  y2=y
  
 // coords under hitbox
 elseif aim =="down" then
  x1=x  			
  y1=y+h
  x2=x+w
  y2=y+h
 end
 
 x1/=8
 x2/=8
 y1/=8
 y2/=8
 
 // check if coords are inside
 // a wall sprite
 if fget(mget(x1,y1), flag)
 or fget(mget(x1,y2), flag)
 or fget(mget(x2,y1), flag)
 or fget(mget(x2,y2), flag) then
  return true // inside wall
 else 
  return false // outside wall
 end
 
 
end
