--gadgets

--torpedos
function torps_setup()
 torps={}
end

function make_torp(x,y)
 local torp={}
 
 torp.x=x
 torp.y=y
 
 torp.w=8
 torp.h=4
 
 if p.flp==true then
  torp.dx=-3
 else
  torp.dx=3
 end
 
 torp.dy=0
 
 if p.flp==true then
  torp.spr=28
 else
  torp.spr=12
 end
 
 add(torps,torp)
end

function update_torp(t)
 if collide_map(t,"left",0)==false
 or collide_map(t,"right",0)==false
 then
  t.x+=t.dx
 else
  del(torps,t)
 end
 t.y+=t.dy
end

function draw_torp(t)
 spr(t.spr,t.x,t.y)
end

--depth charges
function charges_setup()
 charges={}
end

function make_charge(x,y)
 local charge={}
 
 charge.x=x
 charge.y=y
 
 charge.w=8
 charge.h=6
 
 charge.dx=0
 charge.dy=1
 
 if p.flp==true then
  charge.spr=27
 else
  charge.spr=11
 end
 
 add(charges,charge)
end

function update_charge(c)
 c.x+=c.dx
 if (collide_map(c,"down",0)==false)
 then
  c.y+=c.dy
 end
end

function draw_charge(c)
 spr(c.spr,c.x,c.y)
end

--bubbles
function bubbles_setup()
 bubble={}
end

function make_bubble(x,y)
 local bubble={}
 
 bubble.x=x
 bubble.y=y
 
 bubble.w=8
 bubble.h=8

 bubble.dx=0
 bubble.dy=-2
 
 bubble.spr=16
  
 add(bubbles,bubble)
end

function update_bubble(b)
 if collide_map(b,"up",0)==false
 then
  b.x+=b.dx
 else
  del(bubbles,b)
 end
 b.y+=b.dy
end

function draw_bubble(b)
 spr(b.spr,b.x,b.y)
end





